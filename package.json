{
  "name": "vtexy",
  "description": "A developer framework for VTEX Legacy.",
  "version": "0.0.0",
  "author": "Gabriel Nobrega @ganobrega",
  "repository": "ganobrega/vtexy",
  "homepage": "https://github.com/ganobrega/vtexy",
  "bugs": "https://github.com/ganobrega/vtexy/issues",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vtexy",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "sync": {
        "description": "sync data with VTEX"
      },
      "config": {
        "description": "manage vtexy config"
      }
    }
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": {
    "vtexy": "./bin/run"
  },
  "keywords": [
    "oclif"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/browser-sync": "^2.26.1",
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.150",
    "@types/lodash-es": "^4.17.3",
    "@types/ora": "^3.2.0",
    "@types/ramda": "^0.27.4",
    "@types/traverse": "^0.6.32",
    "@types/yup": "^0.28.3",
    "browser-sync": "^2.26.7",
    "conf": "^6.2.4",
    "consola": "^2.12.1",
    "cosmiconfig": "^6.0.0",
    "cross-env": "^7.0.2",
    "cson": "^7.3.0",
    "deepdash": "^5.0.3",
    "deepdash-es": "^5.0.3",
    "device": "^0.3.11",
    "directory-tree": "^2.2.4",
    "dotenv": "^8.2.0",
    "enquirer": "^2.3.5",
    "execa": "^4.0.1",
    "figlet": "^1.4.0",
    "find-up": "^4.1.0",
    "glob": "^7.1.6",
    "hygen": "^5.0.3",
    "i18n": "^0.9.1",
    "isdev": "^1.0.1",
    "jsonc-parser": "^2.2.1",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "log-symbols": "^4.0.0",
    "log-update": "^4.0.0",
    "matcher": "^3.0.0",
    "ora": "^4.0.4",
    "p-pipe": "^3.0.0",
    "posthtml": "^0.13.0",
    "posthtml-parser": "^0.4.2",
    "preact": "^10.4.1",
    "preact-compat": "^3.19.0",
    "query-string": "^6.12.1",
    "slugify": "^1.4.0",
    "terminal-link": "^2.1.1",
    "transformer-proxy": "^0.3.5",
    "traverse": "^0.6.6",
    "ts-node": "^8",
    "tslib": "^1",
    "typescript": "^3.3",
    "url-parse": "^1.4.7",
    "velocity": "^0.7.2",
    "velocityjs": "^2.0.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/jest": "^25.2.1",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "globby": "^10",
    "jest": "^26.0.1",
    "jest-diff": "^26.0.1",
    "nyc": "^14",
    "prettier": "^2.0.5",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ]
}
